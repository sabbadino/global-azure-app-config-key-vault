{ 
  "app-config-refresh-in-seconds": 10,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",
  "SettingsGroup": {
    "FromAppLocalSecret": "local",
    "FromAppSettingsFile": "local",
    "FromAkv": "local",
    "FromAppConfigurationSimple": "local",
    "FromAppConfigurationComplex": {
      "FontSize": "1",
      "UseDefaultRouting": true
    }
  }
}
